{
  "name": "litepicker",
  "version": "2.0.12",
  "description": "Date range picker - lightweight, no dependencies",
  "main": "./dist/litepicker.umd.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "prod": "webpack --config config/webpack.prod.js",
    "dev": "webpack --config config/webpack.dev.js",
    "nocss": "webpack --config config/webpack.nocss.js",
    "plugins": "webpack --config config/webpack.plugins.js && webpack --config config/webpack.plugins.nocss.js",
    "watch": "webpack -w --config config/webpack.dev.js",
    "test": "jest",
    "prepare": "yarn prod && yarn plugins"
  },
  "keywords": [
    "datepicker",
    "date",
    "daterange",
    "date",
    "picker",
    "range",
    "lightweight",
    "no-dependencies"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wakirin/litepicker.git"
  },
  "author": {
    "name": "Rinat G.",
    "email": "litepicker@lvl.ninja",
    "url": "https://lvl.ninja"
  },
  "homepage": "https://litepicker.com",
  "bugs": {
    "url": "https://github.com/wakirin/litepicker/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/eslint-plugin-tslint": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "dts-css-modules-loader": "^2.0.1",
    "eslint": "^8.40.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-jsdoc": "^44.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "parallel-webpack": "^2.6.0",
    "postcss-loader": "^7.3.0",
    "proxy-polyfill": "^0.3.0",
    "sass": "^1.42.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "5.0.4",
    "webpack": "5.x",
    "webpack-cli": "5.1.1",
    "webpack-merge": "5.x"
  },
  "browserslist": [
    "defaults",
    "last 2 versions",
    "not ie < 11",
    "> 1%"
  ],
  "dependencies": {
    "all": "^0.0.0"
  }
}
